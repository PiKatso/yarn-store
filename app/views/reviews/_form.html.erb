<%= render "layouts/errors", :object => @review %>

<%= form_for [@product, @review] do |f| %>
  <%= f.label :author %>
  <%= f.text_field :author %>
<br>
  <%= f.label :content %>
  <%= f.text_field :content %>
<br>
  <!-- <%= f.hidden_field(:user_id, value: current_user.id) %> -->
  <%= f.submit %>
<% end %>
